@page "/policys/successpage/{message}"
@inject NavigationManager NavigationManager

<PageTitle>Success page</PageTitle>
<h1>@Message</h1>

<button @onclick="ToPolicysList">Back</button>

@functions {
    [Parameter]
    public string Message { get; set; }

    private void ToPolicysList()
    {
        NavigationManager.NavigateTo("/policys");
    }
}
