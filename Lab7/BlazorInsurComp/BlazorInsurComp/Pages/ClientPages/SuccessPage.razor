@page "/clients/successpage/{message}"
@inject NavigationManager NavigationManager

<PageTitle>Success page</PageTitle>
<h1>@Message</h1>

<button @onclick= "ToClientsList">Back</button>

@functions{
    [Parameter]
    public string Message{ get; set; }

    private void ToClientsList()
    {
        NavigationManager.NavigateTo("/clients");
    }
}

